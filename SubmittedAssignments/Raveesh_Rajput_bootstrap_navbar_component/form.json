{
  "navbar-component": {
    "formFields": [
      {
        "wrappers": [
          "tabs"
        ],
        "templateOptions": {
          "orientation": "horizontal",
          "navType": "nav nav-tabs"
        },
        "fieldGroup": [
          {
            "key": "config",
            "templateOptions": {
              "label": "Config"
            },
            "fieldGroupClassName": "row",
            "fieldGroup": [
              {
                "key": "carouselClass",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Carousel CSS Class",
                  "placeholder": "carousel carousel-dark"
                }
              },
              {
                "key": "innerClass",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Inner CSS Class",
                  "placeholder": "carousel-inner"
                }
              },
              {
                "key": "imgClass",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Image CSS Class",
                  "placeholder": "d-block w-100 ratio ratio-21x9"
                }
              },
              {
                "key": "captionClass",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Caption CSS Class",
                  "placeholder": "carousel-caption d-none d-md-block mb-4"
                }
              },
              {
                "key": "prevButtonClass",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Previous Button CSS Class",
                  "placeholder": "carousel-control-prev"
                }
              },
              {
                "key": "nextButtonClass",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Next Button CSS Class",
                  "placeholder": "carousel-control-next"
                }
              },
              {
                "key": "indicators",
                "type": "checkbox",
                "className": "col-12",
                "templateOptions": {
                  "label": "Enable Indicators"
                }
              },
              {
                "key": "captions",
                "type": "checkbox",
                "className": "col-12",
                "templateOptions": {
                  "label": "Enable Captions"
                }
              },
              {
                "key": "buttons",
                "type": "checkbox",
                "className": "col-12",
                "templateOptions": {
                  "label": "Enable Buttons"                  
                }
              }
            ]
          },
          {
            "key": "data",
            "type":"repeat",
            "templateOptions": {
              "label": "Data",
              "addText":"Add Data"
            },
            "fieldArray": {
              "fieldGroupClassName": "row",
              "fieldGroup": [
                {
                  "key": "src",
                  "type": "file-manager",
                  "className": "col-12",
                  "templateOptions": {
                    "label": "Image URL"
                  }
                },
                {
                  "key": "alt",
                  "type": "input",
                  "className": "col-12",
                  "templateOptions": {
                    "label": "Alt Text",
                    "placeholder": "Enter alt text"
                  }
                },
                {
                  "key": "caption",
                  "fieldGroup": [
                    {
                      "key": "caption.title",
                      "type": "input",
                      "className": "col-12",
                      "templateOptions": {
                        "label": "Caption Title",
                        "placeholder": "Enter caption title"
                      }
                    },
                    {
                      "key": "caption.subtitle",
                      "type": "input",
                      "className": "col-12",
                      "templateOptions": {
                        "label": "Caption Subtitle",
                        "placeholder": "Enter caption subtitle"
                      }
                    },
                    {
                      "key": "caption.text",
                      "type": "textarea",
                      "className": "col-12",
                      "templateOptions": {
                        "label": "Caption Text",
                        "placeholder": "Enter caption text"
                      }
                    },
                    {
                      "key": "caption.titleLink",
                      "type": "input",
                      "className": "col-12",
                      "templateOptions": {
                        "label": "Caption Title Link",
                        "placeholder": "https://example.com"
                      }
                    }
                  ]
                },
                {
                  "key": "buttons",
                  "type": "repeat",
                  "templateOptions": {
                    "label": "Add Button",
                    "addText":"Add Button"
                  },
                  "fieldArray": {
                    "fieldGroup": [
                      {
                        "key": "text",
                        "type": "input",
                        "className": "col-12",
                        "templateOptions": {
                          "label": "Button Text",
                          "placeholder": "Enter button text"
                        }
                      },
                      {
                        "key": "link",
                        "type": "input",
                        "className": "col-12",
                        "templateOptions": {
                          "label": "Button Link",
                          "placeholder": "Enter button link (URL)"
                        }
                      },
                      {
                        "key": "class",
                        "type": "input",
                        "className": "col-12",
                        "templateOptions": {
                          "label": "Button CSS Class",
                          "placeholder": "Enter button class"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    ],
    "formData": {
      "config ": {
        "carouselClass": "carousel slide carousel-dark",
        "innerClass": "carousel-inner",
        "itemClass": "carousel-item",
        "imgClass": "d-block w-100 ratio ratio-21x9",
        "prevButtonClass": "carousel-control-prev",
        "nextButtonClass": "carousel-control-next",
        "prevIconClass": "carousel-control-prev-icon",
        "nextIconClass": "carousel-control-next-icon",
        "buttonTextClass": "visually-hidden",
        "indicatorClass": "carousel-indicators",
        "indicatorItemClass": "carousel-indicator",
        "activeClass": "active",
        "ride": "carousel",
        "touch": true,
        "slide": true,
        "captionClass": "carousel-caption d-none d-md-block mb-4",
        "captionTitleClass": "carousel-title h1 text-danger text-uppercase text-decoration-none",
        "captionSubTitleClass": "carousel-subtitle h3 text-muted",
        "captionTextClass": "carousel-text fs-4 fw-normal",
        "buttonContainerClass": "carousel-button-container",
        "buttonClass": "btn btn-primary rounded-pill ms-2",
        "indicators": true,
        "captions": true,
        "buttons": true
      },
      "data": [
        {
          "src": "https://via.placeholder.com/800x400?text=First+Slide",
          "alt": "First Slide",
          "caption": {
            "title": "Journey Through the Unknown",
            "subtitle": "Explore the Unseen",
            "text": "Some representative placeholder content for the first slide.",
            "titleLink": "https://example.com/first"
          },
          "buttons": [
            {
              "text": "Learn More",
              "link": "https://example.com/new-button",
              "class": "btn btn-danger  rounded-pill"
            },
            {
              "text": "Get Started",
              "link": "https://example.com/get-started"
            },
            {
              "text": "Contact Us",
              "link": "https://example.com/contact",
              "class": "btn btn-success  rounded-pill"
            }
          ]
        },
        {
          "src": "https://via.placeholder.com/800x400?text=Second+Slide",
          "alt": "Second Slide",
          "caption": {
            "title": "Whispers of the Horizon",
            "subtitle": "Look Beyond",
            "text": "Some representative placeholder content for the second slide.",
            "titleLink": "https://example.com/second"
          },
          "buttons": [
            {
              "text": "Learn More",
              "link": "https://example.com/new-button",
              "class": "btn btn-danger  rounded-pill"
            },
            {
              "text": "Get Started",
              "link": "https://example.com/get-started"
            },
            {
              "text": "Contact Us",
              "link": "https://example.com/contact",
              "class": "btn btn-success  rounded-pill"
            }
          ]
        }
      ]
    }
  }
}