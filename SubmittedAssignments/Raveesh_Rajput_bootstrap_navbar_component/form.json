{
  "navbar-component": {
    "formFields": [
      {
        "wrappers": [
          "tabs"
        ],
        "templateOptions": {
          "orientation": "horizontal",
          "navType": "nav nav-tabs"
        },
        "fieldGroup": [
          {
            "key": "config",
            "templateOptions": {
              "label": "Config"
            },
            "fieldGroupClassName": "row",
            "fieldGroup": [
              {
                "key": "navbar",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Navbar Class",
                  "placeholder": "Enter navbar classes"
                }
              },
              {
                "key": "navbodyClass",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Nav Body Class",
                  "placeholder": "Enter nav body classes"
                }
              },
              {
                "key": "brand",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Brand Class",
                  "placeholder": "Enter brand class"
                }
              },
              {
                "key": "brandlogo",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Brand Logo",
                  "placeholder": "Enter brand logo"
                }
              }
            ]
          },
          {
            "key": "data",
            "type": "repeat",
            "templateOptions": {
              "label": "NavBar Data",
              "addText": "Add navbar item"
            },
            "fieldArray": {
              "fieldGroupClassName": "row",
              "fieldGroup": [
                {
                  "key": "src",
                  "type": "input",
                  "className": "col-12",
                  "templateOptions": {
                    "label": "navbar brand src",
                    "addText": "Enter your logo link src"
                  }
                },
                {
                  "key": "text",
                  "type": "input",
                  "className": "col-12",
                  "templateOptions": {
                    "label": "navbar brand name",
                    "addText": "Enter your brand name"
                  }
                },
                {
                  "key": "href",
                  "type": "input",
                  "className": "col-12",
                  "templateOptions": {
                    "label": "navbar href",
                    "addText": "Enter your href"
                  }
                }
              ]
            }
          }
        ]
      }
    ],
    "formData": {
      "config": {
        "navbar": "navbar navbar-expand-lg navbar-light bg-light",
        "navbodyClass": "container-fluid",
        "brand": "navbar-brand",
        "brandlogo": "logo"
      },
      "data": [
        {
          "src": "/img/bird-logo.avif",
          "text": "BRAND",
          "href": "#"
        }
      ]
    }
  }
}