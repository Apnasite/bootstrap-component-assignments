{
  "carousel-element": {
    "formFields": [
      {
        "wrappers": ["tabs"],
        "templateOptions": {
          "orientation": "horizontal",
          "navType": "nav nav-tabs"
        },
        "fieldGroup": [
          {
            "key": "config",
            "templateOptions": {
              "label": "Config"
            },
            "fieldGroupClassName": "row",
            "fieldGroup": [
              {
                "key": "carouselClass",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Carousel CSS Class",
                  "placeholder": "carousel carousel-dark"
                }
              },
              {
                "key": "innerClass",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Inner CSS Class",
                  "placeholder": "carousel-inner"
                }
              },
              {
                "key": "indicators",
                "type": "checkbox",
                "className": "col-12",
                "templateOptions": {
                  "label": "Enable Indicators"
                }
              }
            ]
          },
          {
            "key": "data",
            "templateOptions": {
              "label": "Data"
            },
            "type": "repeat",
            "templateOptions": {
              "label": "Add Slides"
            },
            "fieldArray": {
              "fieldGroupClassName": "row",
              "fieldGroup": [
                {
                  "key": "src",
                  "type": "file-manager",
                  "className": "col-12",
                  "templateOptions": {
                    "label": "Image URL"
                  }
                },
                {
                  "key": "alt",
                  "type": "input",
                  "className": "col-12",
                  "templateOptions": {
                    "label": "Alt Text",
                    "placeholder": "Enter alt text"
                  }
                },
                {
                  "key": "caption",
                  "fieldGroup": [
                    {
                      "key": "title",
                      "type": "input",
                      "className": "col-12",
                      "templateOptions": {
                        "label": "Caption Title",
                        "placeholder": "Enter caption title"
                      }
                    },
                    {
                      "key": "subtitle",
                      "type": "input",
                      "className": "col-12",
                      "templateOptions": {
                        "label": "Caption Subtitle",
                        "placeholder": "Enter caption subtitle"
                      }
                    },
                    {
                      "key": "text",
                      "type": "textarea",
                      "className": "col-12",
                      "templateOptions": {
                        "label": "Caption Text",
                        "placeholder": "Enter caption text"
                      }
                    }
                  ]
                },
                {
                  "key": "buttons",
                  "type": "repeat",
                  "templateOptions": {
                    "label": "Add Button"
                  },
                  "fieldArray": {
                    "fieldGroup": [
                      {
                        "key": "text",
                        "type": "input",
                        "className": "col-12",
                        "templateOptions": {
                          "label": "Button Text",
                          "placeholder": "Enter button text"
                        }
                      },
                      {
                        "key": "link",
                        "type": "input",
                        "className": "col-12",
                        "templateOptions": {
                          "label": "Button Link",
                          "placeholder": "Enter button link (URL)"
                        }
                      },
                      {
                        "key": "class",
                        "type": "input",
                        "className": "col-12",
                        "templateOptions": {
                          "label": "Button CSS Class",
                          "placeholder": "Enter button class"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    ],
    "formData": {
      "config": {
        "carouselClass": "carousel slide carousel-dark",
        "innerClass": "carousel-inner",
        "indicators": true
      },
      "data": [
        {
          "src": "https://via.placeholder.com/800x400?text=First+Slide",
          "alt": "First Slide",
          "caption": {
            "title": "Journey Through the Unknown",
            "subtitle": "Explore the Unseen",
            "text": "Some representative placeholder content for the first slide."
          },
          "buttons": [
            {
              "text": "Learn More",
              "link": "https://example.com/learn",
              "class": "btn btn-primary"
            }
          ]
        }
      ]
    }
  }
}
