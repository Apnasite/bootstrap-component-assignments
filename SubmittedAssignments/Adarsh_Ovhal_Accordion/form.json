{
  "accordion-element": {
    "formFields": [
      {
        "wrappers": [
          "tabs"
        ],
        "templateOptions": {
          "orientation": "horizontal",
          "navType": "nav nav-tabs"
        },
        "fieldGroup": [
          {
            "key": "config",
            "templateOptions": {
              "label": "Config"
            },
            "fieldGroupClassName": "row",
            "fieldGroup": [
              {
                "key": "animation",
                "type": "checkbox",
                "className": "col-12",
                "templateOptions": {
                  "label": "Enable Animation"
                }
              },
              {
                "key": "isOpen",
                "type": "checkbox",
                "className": "col-12",
                "templateOptions": {
                  "label": "Is Accordion Open"
                }
              },
              {
                "key": "closeOthers",
                "type": "checkbox",
                "className": "col-12",
                "templateOptions": {
                  "label": "Close Others"
                }
              },
              {
                "key": "isFlush",
                "type": "checkbox",
                "className": "col-12",
                "templateOptions": {
                  "label": "Is Flush Style"
                }
              },
              {
                "key": "bgColor",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Background Color",
                  "placeholder": "#f8f9fa"
                }
              },
              {
                "key": "alternateBg",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Alternate Background",
                  "placeholder": "#ffffff"
                }
              },
              {
                "key": "elementClasses",
                "type": "input",
                "className": "col-12",
                "templateOptions": {
                  "label": "Element Classes",
                  "placeholder": "border rounded"
                }
              }
            ]
          },
          {
            "key": "data",
            "type": "repeat",
            "templateOptions": {
              "label": "Accordion Data",
              "addText": "Add Accordion Item"
            },
            "fieldArray": {
              "fieldGroupClassName": "row",
              "fieldGroup": [
                {
                  "key": "heading",
                  "type": "input",
                  "className": "col-12",
                  "templateOptions": {
                    "label": "Accordion Item Heading",
                    "placeholder": "Enter Heading"
                  }
                },
                {
                  "key": "body",
                  "type": "textarea",
                  "className": "col-12",
                  "templateOptions": {
                    "label": "Accordion Item Body",
                    "placeholder": "Enter Body Text"
                  }
                },
                {
                  "key": "table",
                  "type": "repeat",
                  "templateOptions": {
                    "label": "Table Rows",
                    "addText": "Add Table Row"
                  },
                  "fieldArray": {
                    "fieldGroup": [
                      {
                        "key": "srNo",
                        "type": "input",
                        "className": "col-4",
                        "templateOptions": {
                          "label": "Serial Number"
                        }
                      },
                      {
                        "key": "name",
                        "type": "input",
                        "className": "col-4",
                        "templateOptions": {
                          "label": "Name"
                        }
                      },
                      {
                        "key": "email",
                        "type": "input",
                        "className": "col-4",
                        "templateOptions": {
                          "label": "Email"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    ],
    "formData": {
      "config": {
        "animation": true,
        "isOpen": false,
        "closeOthers": true,
        "isFlush": false,
        "bgColor": "#f8f9fa",
        "alternateBg": "#ffffff",
        "elementClasses": "border rounded"
      },
      "data": [
        {
          "heading": "Accordion Item 1",
          "body": "This is the content of the first accordion item.",
          "table": [
            {
              "srNo": 1,
              "name": "Adarsh Ovhal",
              "email": "adarshovhal@gmail.com"
            },
            {
              "srNo": 2,
              "name": "Shirish Panchal",
              "email": "shirish@gmail.com"
            }
          ]
        },
        {
          "heading": "Accordion Item 2",
          "body": "This is the content of the second accordion item.",
          "table": []
        }
      ]
    }
  }
}
